FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/air";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -2 0 0 0 0 ];

internalField   uniform 0.1;

boundaryField
{

    wall
    {
        type            compressible::kqRWallFunction;
        value           uniform 0.1;
    }


    air_to_solid
    {
        type            compressible::kqRWallFunction;
        value           uniform 0.1;
    }
    empty_air
    {
        type            empty;
    }
}


